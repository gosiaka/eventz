<h1>Sign In</h1>

<%# formularz dla session: session nie ma modelu, więc używamy url: url_to_post_to %>

<%= form_with(url: session_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email, autofocus: true %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign In"  %>
<% end %>
